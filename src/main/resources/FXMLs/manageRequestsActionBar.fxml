<AnchorPane prefHeight="300.0" prefWidth="1750.0" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.manlyminotaurs.viewControllers.manageRequestsActionBarController">
   <children>
      <Label layoutX="525.0" layoutY="17.0" text="Open Requests">
