            <Font size="25.0" />
         </font>
      </Label>
      <Label layoutX="1373.0" layoutY="16.0" text="Closed Requests">
         <font>
            <Font size="27.0" />
         </font>
      </Label>
      <Label layoutX="878.0" layoutY="17.0" text="Request  Details">
         <font>
            <Font size="27.0" />
         </font>
      </Label>
      <TableView fx:id="tblOpenRequests" layoutX="453.0" layoutY="68.0" prefHeight="200.0" prefWidth="312.0" />
      <Button fx:id="btnCompleteRequest" layoutX="178.0" layoutY="99.0" mnemonicParsing="false" onAction="#promptCompleteRequest" prefHeight="36.0" prefWidth="227.0" text="Complete Request" />
      <Button fx:id="btnDeleteRequest" layoutX="178.0" layoutY="200.0" mnemonicParsing="false" onAction="#promptDeleteRequest" prefHeight="36.0" prefWidth="227.0" text="Delete Request" />
