<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="1080.0" prefWidth="1920.0" xmlns="http://javafx.com/javafx/9.0.4" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.manlyminotaurs.viewControllers.deleteNodeConfirmationController">
   <children>
      <Pane layoutX="660.0" layoutY="300.0" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: gray;">
         <children>
            <Label fx:id="lblHeader" alignment="CENTER" layoutX="121.0" layoutY="46.0" prefHeight="75.0" prefWidth="340.0" text="Are you sure you want to remove:">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label fx:id="lblLongName" alignment="CENTER" layoutX="110.0" layoutY="117.0" prefHeight="98.0" prefWidth="399.0" text="Node Long Name">
               <font>
                  <Font size="25.0" />
               </font>
            </Label>
            <Button fx:id="btnYes" layoutX="315.0" layoutY="265.0" mnemonicParsing="false" onAction="#yes" prefHeight="93.0" prefWidth="226.0" text="Yes" />
            <Button fx:id="btnNo" layoutX="50.0" layoutY="265.0" mnemonicParsing="false" onAction="#no" prefHeight="93.0" prefWidth="226.0" text="No" />
         </children>
      </Pane>
   </children>
</AnchorPane>
